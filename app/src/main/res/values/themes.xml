<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.CanHub" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="Theme.CanHub" parent="Base.Theme.CanHub">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:statusBarColor">#51FFFFFF</item>
        <item name="navigationIcon">@color/azul</item>
        <item name="bottomNavigationStyle">@style/BottomNavigation</item>
    </style>

    <style name="BottomNavigation">
        <item name="itemIconTint">@color/azul</item>
        <item name="background">@color/white</item>
        <item name="itemTextColor">@color/azul</item>
    </style>

    <style name="Theme.CanHub.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.CanHub.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.CanHub.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>